{
  "name": "vitest-pool-assemblyscript",
  "version": "0.0.1",
  "description": "AssemblyScript testing with Vitest - Simple, fast, familiar, AS-native, with full coverage output",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./assembly": {
      "types": "./assembly/index.ts",
      "default": "./assembly/index.ts"
    }
  },
  "files": [
    "dist",
    "assembly"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "tc": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@types/source-map": "^0.5.2",
    "@vitest/runner": "^3.2.4",
    "assemblyscript": "^0.28.9",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "binaryen": "^124.0.0",
    "birpc": "^2.4.0",
    "source-map": "^0.7.6",
    "tinypool": "^2.0.0"
  }
}
